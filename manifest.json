{
  "manifest_version": 3,
  "name": "Memoria",
  "version": "1.0",
  "description": "An AI-powered way to explore your browsing history.",
  "permissions": [
    "history",
    "favicon",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "history": "index.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}